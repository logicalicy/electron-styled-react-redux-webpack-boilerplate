{
  "name": "electron-styled-react-redux-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "index.js",
  "scripts": {
    "dev": "sleep 2 && ENV=dev electron .",
    "server": "babel-node server.js",
    "start": "concurrently -k 'npm run server' 'npm run dev'",
    "build": "webpack --config webpack.config.prod.js && electron-packager . electron-styled-react-redux-webpack-boilerplate --platform=darwin --arch=all --overwrite"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "repository": "https://github.com/logicalicy/electron-styled-react-redux-webpack-boilerplate",
  "keywords": [
    "Electron",
    "styled",
    "react",
    "redux",
    "router",
    "webpack",
    "boilerplate"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "concurrently": "^3.5.0",
    "electron": "~15.5.5",
    "electron-packager": "^8.7.2",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "lodash": "^4.17.4",
    "menubar": "^5.2.3",
    "polished": "^1.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-actions": "^2.0.3",
    "redux-saga": "^0.15.4",
    "styled-components": "^2.1.0"
  }
}
